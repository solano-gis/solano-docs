{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "SAGE Documentation",
  "colors": {
    "primary": "#00475F",
    "light": "#0070A6",
    "dark": "#003a4e"
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Introduction",
        "pages": [
          "index",
          "architecture",
          "capabilities"
        ]
      },
      {
        "group": "Services",
        "pages": [
          "services/sage-web",
          "services/sage-gis",
          "services/sage-gov",
          "services/sage-map"
        ]
      },
      {
        "group": "Features",
        "pages": [
          "knowledge",
          "services/oblique-imagery",
          "services/lidar-elevation",
          "services/county-code",
          "services/data-visualization",
          "services/pdf-export",
          "services/hires-tiles"
        ]
      },
      {
        "group": "Integration",
        "pages": [
          "integration/mcp",
          "integration/map-protocol"
        ]
      },
      {
        "group": "Governance",
        "pages": [
          "governance",
          "evaluation"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/solano-gis",
          "icon": "github"
        },
        {
          "anchor": "Live Demo",
          "href": "https://sage-web-sand.vercel.app",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "GitHub",
      "href": "https://github.com/solano-gis"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/solano-gis"
    }
  }
}
